<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:beans="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<context:annotation-config />
	
	<beans:bean id="userService" class="com.ssmksh.closestack.service.UserServiceImpl"></beans:bean>
	<beans:bean id="userDAO" class="com.ssmksh.closestack.dao.UserDAOImpl"></beans:bean>
	
	<beans:bean id="instanceDAO" class="com.ssmksh.closestack.dao.InstanceDAOImpl"></beans:bean>
	<beans:bean id="instanceService" class="com.ssmksh.closestack.service.InstanceServiceImpl"></beans:bean>
	
	<beans:bean id="flavorDAO" class="com.ssmksh.closestack.dao.FlavorDAOImpl"></beans:bean>
	
	
</beans:beans>
